{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Cleaner For Chrome",
  "version": "6.3",
  "description": "An effective tool for cleaning your browser and monitoring system resources",
  "permissions": [
    "browsingData",
    "storage",
    "downloads",
    "history"
  ],
  "optional_permissions": [
    "system.cpu",
    "system.memory",
    "topSites"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}