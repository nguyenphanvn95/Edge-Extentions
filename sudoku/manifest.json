{
  "manifest_version": 3,
  "name": "Sudoku Master Pro",
  "version": "2.0.0",
  "description": "Complete Sudoku game with save/load, statistics, dark mode, daily challenges, themes, and import/export",
  "permissions": ["storage", "alarms"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "service_worker": "js/background.js"
  }
}
